                android:dashWidth="@dimen/_5sdp"
                android:width="@dimen/_2sdp"
                android:color="#0087eb" />
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />
        </shape>
    </item>
</layer-list>
